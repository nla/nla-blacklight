<header class="mb-3 mb-lg-5">
  <nav class="navbar navbar-expand-md navbar-dark bg-dark topbar" role="navigation">
    <div class="<%= container_classes %>">
      <%= logo_link %>
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-bs-toggle="collapse" data-target="#user-util-collapse" data-bs-target="#user-util-collapse" aria-controls="user-util-collapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse justify-content-md-end mt-3 mb-lg-n5" id="user-util-collapse">
        <%= render 'shared/user_util_links' %>
      </div>
    </div>
  </nav>
  <div class="navbar navbar-dark bg-dark bottombar">
    <div class="container">
      <%= content_for(:page_header) || t('blacklight.application_name') %>
    </div>
  </div>
</header>

<% if details.enumeration.present? %>
<dt class="col-12 col-md-4 text-md-right"><%= t("requesting.label.map_name") %></dt>
<dd class="col col-md-8"><%= details.enumeration %></dd>
<% end %>

<% if details.yearCaption.present? %>
  <dt class="col-12 col-md-4 text-md-right"><%= t("requesting.label.year") %></dt>
  <dd class="col col-md-8">
    <ul class="list-unstyled">
      <% details.yearCaption.each do |year| %>
        <li><%= year %></li>
      <% end %>
    </ul>
  </dd>
<% end %>
